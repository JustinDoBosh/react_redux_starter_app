{
  "name": "starter_app",
  "version": "0.0.1",
  "description": "react redux starter app",
  "main": "webpack.config.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production webpack",
    "build-windows": "set NODE_ENV=production& webpack",
    "dev": "NODE_ENV=development nodemon ./src/server/index.js --exec babel-node --presets es2015,stage-0 --ignore ./src/client --ignore ./dist ",
    "dev-windows": "set NODE_ENV=development& nodemon ./src/server/index.js --ignore ./src/client --ignore ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": ["syntax-async-functions","transform-regenerator"]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Justin DoBosh",
  "license": "ISC",
  "homepage": "",
